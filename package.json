  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "dev-hapi": "concurrently --kill-others \"MAIN=staging node build/dev-server.js\" \"npm run hapi-dev\"",
    "build": "node --no-deprecation --no-warnings build/build.js",
    "build-staging": "MAIN=staging node --no-deprecation --no-warnings build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "test-e2e": "nightwatch || true",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "hapi-dev": "node server/index.js",
    "hapi": "NODE_ENV=production node server/index.js",
    "hapi-windows": "SET NODE_ENV=production&&node server/index.js"
  },
